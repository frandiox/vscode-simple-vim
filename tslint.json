{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [
            true,
            "spaces",
            2
        ],
        "semicolon": "never",
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "ordered-imports": false,
        "curly": [
            true,
            "ignore-same-line"
        ],
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "no-console": false,
        "interface-over-type-literal": false,
        "array-type": false
    },
    "rulesDirectory": []
}